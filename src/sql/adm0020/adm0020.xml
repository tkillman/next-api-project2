<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="adm0020.adm0020">  
  <select id="adm0020GetOrderList" resultMap="HashMap">
    <![CDATA[
      /*webadmin-adm0020 adm0020GetOrderList*/
      select PASSWD
            , NAME
            , EMAIL
            , PHONE
            , STATUS
      from member
      ]]>
      <if test=NAME != ''>
      where NAME = :NAME
      </if>
  </select>
</mapper>